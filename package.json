{
  "name": "wetter-anzeige-app",
  "version": "1.0.0",
  "description": "Eine einfache Full-Stack-Webanwendung zur Anzeige von Wetterdaten für verschiedene Städte.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently "npm run client" "nodemon server.js""
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "cors": "^2.8.5",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "concurrently": "^5.3.0"
  }
}